<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="cf_AssignValuesFromHeader">
    <AssignVariable>
        <Name>senderid</Name>
        <Ref>request.header.senderid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>channelid</Name>
        <Ref>request.header.channelid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>applicationid</Name>
        <Ref>request.header.applicationid</Ref>
    </AssignVariable>
	<AssignVariable>
        <Name>applicationuserid</Name>
        <Ref>request.header.applicationuserid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>activityid</Name>
        <Ref>request.header.activityid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>interactionid</Name>
        <Ref>request.header.interactionid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>sessionid</Name>
        <Ref>request.header.sessionid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>userid</Name>
        <Ref>request.header.userid</Ref>
    </AssignVariable>
   <AssignVariable>
        <Name>dealercode</Name>
        <Ref>request.header.dealercode</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>masterDealerCode</Name>
        <Ref>request.header.masterdealercode</Ref>
    </AssignVariable>
	
    <AssignVariable>
        <Name>workflowid</Name>
        <Ref>request.header.workflowid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>storeid</Name>
        <Ref>request.header.storeid</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>disableentitlementsflow</Name>
        <Ref>request.header.disableentitlementsflow</Ref>
    </AssignVariable>
	
	<AssignVariable>
        <Name>servicetransactionid</Name>
        <Ref>request.header.servicetransactionid</Ref>
    </AssignVariable>
	
    <!-- Target System name -->
    <AssignVariable>
        <Name>capi.target.route.system</Name>
        <Value>U2</Value>
		<!-- Assign header value if exists else default is U2-->
		<Ref>request.header.targetroutesystem</Ref>
    </AssignVariable>
	
    <AssignVariable>
        <Name>cacheaccessutilprotocol</Name>
        <Ref>request.header.cacheAccessUtilProtocol</Ref>
    </AssignVariable>
    
    <AssignVariable>
        <Name>cacheaccessutilappauthorization</Name>
        <Ref>request.header.cacheAccessUtilAppAuthorization</Ref>
    </AssignVariable>
    <AssignVariable>
		<Name>authcustomerid</Name>
		<Ref>request.header.authcustomerid</Ref>
	</AssignVariable>
	<AssignVariable>
		<Name>authfinancialaccountid</Name>
		<Ref>request.header.authfinancialaccountid</Ref>
	</AssignVariable>
	<AssignVariable>
		<Name>authlineofserviceid</Name>
		<Ref>request.header.authlineofserviceid</Ref>
	</AssignVariable>
	<AssignVariable>
		<Name>requestHost</Name>
		<Ref>request.header.Host</Ref>
	</AssignVariable>
	
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
