<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="getShippingOptions_AssignRoutingVariable">

    <!-- Target Service name -->
    <AssignVariable>
        <Name>capi.target.route.service</Name>
        <Value>CartWSIL</Value>
    </AssignVariable>

    <!-- Target  Action name -->    
    <AssignVariable>
        <Name>capi.target.route.operation</Name>
        <Value>getShippingOptions</Value>
    </AssignVariable>
	
	<AssignVariable>
        <Name>flow.cart.verb</Name>
        <Value>GET</Value>
    </AssignVariable>
	
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>